    {{/if}}

    {{#if page.title }}
        <meta content="{{ page.title }} | {{ site.name }}" property="og:title">
        <meta content="{{ page.title }} | {{ site.name }}" property="twitter:title">
        <title>{{ page.title }} | {{ site.name }}</title>
    {{else}}
        <meta content="{{ site.seo_title }} | {{ site.name }}" property="og:title">
        <meta content="{{ site.seo_title }} | {{ site.name }}" property="twitter:title">
        <title>{{ site.seo_title }} | {{ site.name }}</title>

<body>
  <div 
    data-animation="over-left" 
    data-collapse="medium" 
    data-duration="400" 
    data-easing="ease" 
    data-easing2="ease" 
    data-doc-height="1" 
    role="banner" 
    class="navbar-second navbar-home-2 w-nav{{#if dark_bg }} dark-bg{{/if}}"
  >
    <div class="base-container home-2 w-container">
      <nav role="navigation" class="nav-menu-content w-nav-menu">
        <div class="tablet-menu-logo">
          <a href="/" aria-current="page" class="logo-tablet-link w-inline-block w--current">
            <img 
              src="https://res.cloudinary.com/wilki-digital-cloudinary/image/upload/v1691179674/{{ site.logo }}" 
              loading="lazy" 
              alt="{{ site.name }} logo" 
              class="image-20"
              width="175"
              height="auto"
            />
          </a>
          <div class="button-close w-nav-button">
            <img 
              src="/images/round_close_white_48dp.webp" 
              loading="lazy" 
              width="30" 
              height="30" 
              alt="close icon" 
              class="image-close"
            />
          </div>
        </div>

        <a href="/" aria-current="page" class="logo-menu w-inline-block w--current">
          <img 
            src="https://res.cloudinary.com/wilki-digital-cloudinary/image/upload/v1690085101/{{ site.logo }}"
            loading="lazy" 
            width="Auto" 
            height="Auto" 
            alt="{{ site.name }} logo" 
            class="image-nav-logo"
          />
        </a>

        <div class="navigation-link">
          {{#each menus.main_menu.links }}
            {{#if this.links}}
              <div data-hover="true" data-delay="0" data-w-id="0fcf8eb7-f2f0-03a0-f67a-78e583b68d8c" class="nav-dropdown margin w-dropdown">
                <div class="nav-dropdown-toggle-2 none-padding w-dropdown-toggle">
                  <a href="{{ this.url }}" class="nav-item-title-2 dark-text">{{ this.title }}</a>
                  <div class="arrow-bottom-drop dark-text"></div>
                </div>
                <nav class="nav-dropdown-list-second w-dropdown-list">
                  <div class="megamenu-dropdown-wrapper justify">
                    <div class="megamenu-dropdown-wrap-2">
                      <div class="nav-dropdown-link-wrapper">
                        {{#each this.links}}
                          <a href="{{ this.url }}" class="nav-dropdown-link-2 home-2-white w-dropdown-link"><span class="nav-dropdowm-link-line-2"> </span>{{ this.title }}</a>
                        {{/each}}
                      </div>
                    </div>
                  </div>
                </nav>
              </div>
            {{else}}
              <a href="{{ this.url }}" class="link navbar-link">{{ this.title }}</a>
            {{/if}}
          {{/each}}
        </div>

        <div class="button-block-nav">
          <a href="tel:{{ site.phone_number }}" class="primary-button margin-left-28px navbar-cta w-button">{{ site.phone_number }}</a>
        </div>
      </nav>

      <a href="/" id="w-node-_0fcf8eb7-f2f0-03a0-f67a-78e583b68da7-83b68d7d" aria-current="page" class="link-logo w-inline-block w--current">
        <img 
          src="https://res.cloudinary.com/wilki-digital-cloudinary/image/upload/v1691179674/{{ site.logo }}"
          loading="lazy" 
          alt="{{ site.name }} Logo" 
          class="image-nav-logo"
        />
      </a>

      <div class="menu-button-2 invert-color w-nav-button">
        <img src="/images/Burger-button.webp" loading="lazy" width="30" alt="" class="image-18">
      </div>
    </div>
  </div>

  {{{ body }}}

  <a href="tel:{{ site.phone_number }}" class="link-block-2 w-inline-block">
    <img 
      src="/images/64503a7708b85659341618cf_Phone-icon-big-white.png" 
      loading="lazy" 
      alt="phone icon" 
      class="image-19"
    />
  </a>

  <div class="footer">
    <div class="base-container w-container">
      <div class="footer-content">
        <div class="footer-wrapper">
          <div class="footer-brand-wrapper">
            <div class="footer-logo-wrapper">
              <a href="/" class="footer-brand w-nav-brand">
                <img 
                  src="https://res.cloudinary.com/wilki-digital-cloudinary/image/upload/v1691179674/{{ site.logo }}" 
                  loading="lazy" 
                  height="30" 
                  alt="{{ site.name }} logo on footer" 
                  class="footer-logo"
                />
              </a>
              <p class="footer-brand-description">We&#x27;re committed to giving you best tree services and care in South Florida: Hollywood, Davie, Sunrise, Southwest Ranches, and Plantation.</p>
            </div>
          </div>
          <div class="div-block-7">
            <div class="footer-links-wrapper">
              <h5 class="white-text footer-head">{{ menus.footer_menu_one.title }}</h5>
              {{#each menus.footer_menu_one.links }}
                <a href="{{ this.url }}" aria-current="page" class="footer-link w--current">{{ this.title }}</a>
              {{/each}}
            </div>
            <div class="footer-links-wrapper">
              <h5 class="white-text footer-head">{{ menus.footer_menu_two.title }}</h5>
              {{#each menus.footer_menu_two.links }}
                <a href="{{ this.url }}" aria-current="page" class="footer-link w--current">{{ this.title }}</a>
              {{/each}}
            </div>
            <div class="footer-links-wrapper">
              <h5 class="white-text footer-head">{{ menus.footer_menu_three.title }}</h5>
              {{#each menus.footer_menu_three.links }}
                <a href="{{ this.url }}" aria-current="page" class="footer-link w--current">{{ this.title }}</a>
              {{/each}}
            </div>
          </div>
        </div>
      </div>
      <div class="footer-social-wrapper">
        <div class="footer-social-icons-wrapper centered">
          <a href="{{ site.facebook }}" target="_blank" class="footer-social-icon"></a>
          <a href="{{ site.instagram }}" target="_blank" class="footer-social-icon"></a>
          <a href="{{ site.twitter }}" target="_blank" class="footer-social-icon"></a>
          <a href="{{ site.pinterest }}" target="_blank" class="footer-social-icon"></a>
          <a href="{{ site.linkedin }}" target="_blank" class="footer-social-icon"></a>
          <a href="{{ site.yelp }}" target="_blank" class="footer-social-icon"></a>
        </div>
        <div class="block-link-adress">
          <div class="text-footer">{{ site.address }}</div>
          <a href="tel:{{ site.phone_number }}" class="footer-link none-padding"><strong>{{ site.phone_number }}</strong></a>
        </div>
      </div>
      <div class="footer-rights-wrapper">
        <div class="footer-copyright">© {{ site.name }}. All Rights Reserved 2023. </div>
      </div>
    </div>
